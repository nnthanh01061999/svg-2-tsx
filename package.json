{
  "name": "svg-2-tsx",
  "displayName": "SVG 2 TSX",
  "description": "Quick convert SVG to optimized TSX component file",
  "version": "0.0.1",
  "publisher": "nnthanh01061999",
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/nnthanh01061999/svg-2-tsx"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:svg-2-tsx.optimizeAllSvgInFolder",
    "onCommand:svg-2-tsx.optimizeSingleTsxFile",
    "onCommand:svg-2-tsx.svgToReactComponent",
    "onCommand:svg-2-tsx.startOptimizationServer",
    "onCommand:svg-2-tsx.stopOptimizationServer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "svg-2-tsx.svgToReactComponent",
        "title": "S2T: SVG to React Component"
      },
      {
        "command": "svg-2-tsx.startOptimizationServer",
        "title": "S2T: Start SVG Optimization Server"
      },
      {
        "command": "svg-2-tsx.stopOptimizationServer",
        "title": "S2T: Stop SVG Optimization Server"
      },
      {
        "command": "svg-2-tsx.optimizeAllSvgInFolder",
        "title": "S2T: Optimize All SVG in Folder"
      },
      {
        "command": "svg-2-tsx.optimizeSingleTsxFile",
        "title": "S2T: Optimize SVG in File"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "svg-2-tsx.optimizeAllSvgInFolder",
          "group": "navigation@1",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "svg-2-tsx.optimizeSingleTsxFile",
          "group": "navigation@2",
          "when": "resourceExtname == .tsx"
        }
      ],
      "file/context": [
        {
          "command": "svg-2-tsx.optimizeSingleTsxFile",
          "group": "navigation@1",
          "when": "resourceExtname == .tsx"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Svg 2 Tsx configuration",
      "properties": {
        "svg2tsx.iconPaths": {
          "type": "object",
          "default": {
            "Outline": "src/components/Common/Icon/icons/outline",
            "Fill": "src/components/Common/Icon/icons/fill",
            "Color": "src/components/Common/Icon/icons/color",
            "3D": "src/components/Common/Icon/icons/3d"
          },
          "description": "Paths for different icon types"
        },
        "svg2tsx.replaceColor": {
          "type": "object",
          "default": {
            "color": "#2B2B2B",
            "type": [
              "Fill",
              "Outline"
            ]
          },
          "description": "Replace this color with currentColor"
        },
        "svg2tsx.autoExportModule": {
          "type": "boolean",
          "default": true,
          "description": "Auto export module"
        },
        "svg2tsx.svgOptimize": {
          "type": "object",
          "default": {
            "url": "http://localhost:3600/api/optimize",
            "enabled": true
          },
          "description": "URL for SVG optimization tool"
        },
        "svg2tsx.optimizationServerPort": {
          "type": "number",
          "default": 3600,
          "description": "Port for the SVG optimization server"
        },
        "svg2tsx.svgOptimizeConfig": {
          "type": "object",
          "default": {
            "multipass": true,
            "plugins": [
              {
                "name": "preset-default",
                "params": {
                  "overrides": {
                    "removeViewBox": false,
                    "removeTitle": false,
                    "removeDesc": false,
                    "removeDoctype": true,
                    "removeXMLProcInst": true,
                    "removeComments": true,
                    "removeMetadata": true,
                    "removeEditorsNSData": true,
                    "cleanupAttrs": true,
                    "mergeStyles": true,
                    "inlineStyles": true,
                    "minifyStyles": true,
                    "cleanupIDs": true,
                    "removeUselessDefs": true,
                    "convertPathData": true,
                    "convertColors": true,
                    "removeUnknownsAndDefaults": true,
                    "removeUselessStrokeAndFill": true,
                    "removeEnableBackground": true,
                    "removeHiddenElems": true,
                    "removeEmptyText": true,
                    "convertShapeToPath": true,
                    "moveElemsAttrsToGroup": true,
                    "moveGroupAttrsToElems": true,
                    "collapseGroups": true,
                    "convertEllipseToCircle": true,
                    "convertTransform": true,
                    "removeEmptyAttrs": true,
                    "removeEmptyContainers": true,
                    "mergePaths": true,
                    "cleanupNumericValues": true,
                    "sortAttrs": true,
                    "sortDefsChildren": true,
                    "removeDimensions": true
                  }
                }
              }
            ]
          },
          "description": "Config for SVG optimization"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package",
    "build": "npm run compile && npm run package"
  },
  "dependencies": {
    "svgo": "^2.6.1",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17"
  }
}
